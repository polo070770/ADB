<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.model.Dades_entrega" table="dades_entrega"/>
  
  <property name="carrer" column="CARRER"/>
  <property name="poblacio" column="POBLACIO"/>
  <property name="provincia" column="PROVINCIA"/>
  <property name="codi_postal" column="CODI_POSTAL"/>
  <property name="pais" column="PAIS"/>
  
  <many-to-one name="client" class="com.model.Client">
      <column name="ID_CLIENT"/>
  </many-to-one>
  
  
</hibernate-mapping>
